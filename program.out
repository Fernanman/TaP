; ModuleID = 'TaP'
source_filename = "TaP"

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1
@listlit = global [2 x i32] [i32 1, i32 2]

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %ba = alloca i32*, align 8
  store i32* getelementptr inbounds ([2 x i32], [2 x i32]* @listlit, i32 0, i32 0), i32** %ba, align 8
  %ba1 = load i32*, i32** %ba, align 8
  %at = getelementptr inbounds i32, i32* %ba1, i32 1
  %load_elem = load i32, i32* %at, align 4
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt, i32 0, i32 0), i32 %load_elem)
  ret i32 0
}
